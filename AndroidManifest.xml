        <activity
            android:name="cycki.fragmenttest.ItemListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cycki.fragmenttest.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName="cycki.fragmenttest.ItemListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
